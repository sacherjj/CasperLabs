{
  "name": "server",
  "version": "0.1.0",
  "description": "CasperLabs Explorer Server",
  "main": "dist/server.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "dev": "nodemon -e ts --watch src --exec \"npm run build && npm run start\"",
    "start:both": "concurrently \"npm run dev\" \"cd ../ui && npm start\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^12.0.10",
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express-jwt": "0.0.42",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "jwks-rsa": "^1.5.1"
  }
}
